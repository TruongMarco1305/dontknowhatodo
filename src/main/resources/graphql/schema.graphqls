type Query {
    getMe: UserType
}

type Mutation {
    updateProfile(data: UpdateProfileInput!): UserType
}

type UserType {
    id: String!
    username: String!
    email: String!
    name: String!
    roles: String!
    imageUrl: String
    bio: String
    company: String
}

input UpdateProfileInput {
    name: String
    bio: String
    company: String
}